{% extends 'admin/index.html.twig' %}
{% block title %}Region{% endblock %}
{% block h1 %}<h1>Region</h1>{% endblock %}
{% block right %}

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ region.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ region.name }}</td>
            </tr>
            <tr>
                <th>Region_number</th>
                <td>{{ region.regionNumber }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ region.enabled ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('region_index') }}">back to list</a>

    <a href="{{ path('region_edit', {'id': region.id}) }}">edit</a>

    {{ include('admin/region/_delete_form.html.twig') }}
{% endblock %}
