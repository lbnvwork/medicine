{% extends 'admin/index.html.twig' %}

{% block title %}RiskFactor{% endblock %}
{% block h1 %}<h1>RiskFactor</h1>{% endblock %}
{% block right %}
    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ risk_factor.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ risk_factor.name }}</td>
        </tr>
        <tr>
            <th>Scores</th>
            <td>{{ risk_factor.scores }}</td>
        </tr>
        <tr>
            <th>Enabled</th>
